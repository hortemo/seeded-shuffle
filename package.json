{
  "name": "@hortemo/seeded-shuffle",
  "version": "1.0.0",
  "description": "Deterministic, seed-based shuffle for JS/TS.",
  "license": "MIT",
  "author": "Henrik Hortemo",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hortemo/seeded-shuffle.git"
  },
  "bugs": {
    "url": "https://github.com/hortemo/seeded-shuffle/issues"
  },
  "homepage": "https://github.com/hortemo/seeded-shuffle#readme",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts --clean",
    "test": "vitest run",
    "lint": "tsc --noEmit"
  },
  "dependencies": {
    "seedrandom": "^3.0.5"
  },
  "devDependencies": {
    "@types/seedrandom": "^3.0.5",
    "tsup": "^8.5.0",
    "typescript": "^5.6.3",
    "vitest": "^3.2.4"
  },
  "engines": {
    "node": ">=18"
  },
  "keywords": [
    "shuffle",
    "random",
    "seed",
    "deterministic",
    "fisher-yates",
    "typescript"
  ]
}
